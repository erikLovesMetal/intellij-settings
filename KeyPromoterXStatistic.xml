<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>CloseContent</IdeaActionID>
          <ShortCut>'⌘F4' or '⌘W'</ShortCut>
          <Description>Close</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <ShortCut>'⇧⌘\'</ShortCut>
          <Description>Database</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>24</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'slack_service [bootR...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧F10'</ShortCut>
          <Description>Run 'slack_service [bootR...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run...">
        <Item>
          <IdeaActionID>ChooseRunConfiguration</IdeaActionID>
          <ShortCut>'⌥⇧F10'</ShortCut>
          <Description>Run...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'SlashDeathServiceApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'SlashDeathServiceApplication'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'slack_service [bootRun]'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'slack_service [bootRun]'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>