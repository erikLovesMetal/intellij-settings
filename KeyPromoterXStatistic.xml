<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>CloseContent</IdeaActionID>
          <ShortCut>'⌘F4' or '⌘W'</ShortCut>
          <Description>Close</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Value">
        <Item>
          <IdeaActionID>XDebugger.CopyValue</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy Value</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <ShortCut>'⇧⌘\'</ShortCut>
          <Description>Database</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘5'</ShortCut>
          <Description>Debug</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'slack_service [bootR...'">
        <Item>
          <IdeaActionID>DebugClass</IdeaActionID>
          <ShortCut>'⌃⇧F9'</ShortCut>
          <Description>Debug 'slack_service [bootR...'</Description>
          <Count>14</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
