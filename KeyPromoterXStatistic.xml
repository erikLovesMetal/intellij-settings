<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>CloseContent</IdeaActionID>
          <ShortCut>'⌘F4' or '⌘W'</ShortCut>
          <Description>Close</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <ShortCut>'⇧⌘\'</ShortCut>
          <Description>Database</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>17</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run...">
        <Item>
          <IdeaActionID>ChooseRunConfiguration</IdeaActionID>
          <ShortCut>'⌥⇧F10'</ShortCut>
          <Description>Run...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'SlashDeathServiceApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'SlashDeathServiceApplication'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>